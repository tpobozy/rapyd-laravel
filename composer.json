{
    "name": "tpobozy/rapyd-laravel",
    "description": "crud widgets for laravel, to make an admin in few lines of code",
    "homepage": "https://github.com/bretto36/rapyd-laravel",
    "keywords": ["laravel", "crud", "rapyd", "administrator", "admin", "backend", "datatable", "form"],
    "authors": [
        {
            "name": "zofe",
            "email": "felice.ostuni@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "~7.0|~8.0|~9.0|~10.0|~11.0|~12.0",
        "laravelcollective/html": "dev-master as 6.5.0",
        "intervention/image": "^2.0"
    },
    "suggest": {
        "baum/baum": "Allows to use the DataTree widget"
    },
    "autoload": {
        "classmap": [
            "migrations"
        ],
        "psr-4": {
            "Zofe\\Rapyd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zofe\\Rapyd\\RapydServiceProvider"
            ]
        }
    },    
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tpobozy/laravel-collective-html"
        }
    ],
    "require-dev": {
        "illuminate/support": "*",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
