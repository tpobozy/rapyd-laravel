{
    "name": "bretto36/rapyd-laravel",
    "description": "crud widgets for laravel, to make an admin in few lines of code",
    "homepage": "https://github.com/bretto36/rapyd-laravel",
    "keywords": ["laravel", "crud", "rapyd", "administrator", "admin", "backend", "datatable", "form"],
    "authors": [
        {
            "name": "zofe",
            "email": "felice.ostuni@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2|>=8.0",
        "illuminate/support": "~7.0|~8.0|~9.0",
        "laravelcollective/html": "^6.0.0",
        "intervention/image": "2.*",
        "zofe/burp": "^3.1.3"
    },
    "suggest": {
        "baum/baum": "Allows to use the DataTree widget"
    },
    "autoload": {
        "classmap": [
            "migrations"
        ],
        "psr-4": {
            "Zofe\\Rapyd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zofe\\Rapyd\\RapydServiceProvider"
            ]
        }
    },
    "require-dev": {
        "illuminate/support": "~8.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
